<div class="data-header-third-column">
    <div class="row">
        {% verbatim %}
            <div id="node-header" class="col s8">
                <div class="node-selector">{{selected}}</div>
                <span v-if="crumbs">
                    <span class="fil">
                    <a class="breadcrumb" v-for="crumb in crumbs">{{crumb}}</a>
                    </span>
                </span>
            </div>
            {% endverbatim %}
        <div class="col s4">
            <img data-activates="pref-option" src="/img/05-Preferences.svg" class="image-right pref-option-style">
            <img src="/img/04-export.svg" class="image-right" alt="">
            <img data-activates="team-option" src="/img/03-team.svg" class="image-right team-option-style">
            <img src="/img/02-pencil.svg" class="image-right" alt="">
        </div>
        <!-- Dropdown Structure -->
        <ul id='pref-option' class='dropdown-content'>
            <li class="option-node"><a class="dropdown-menu" href="/signOut">Logout</a></li>
        </ul>
        <ul id='team-option' class='dropdown-content'>
            <li class="option-node"><a id="invite-people-btn" class="dropdown-menu">Invite</a></li>
        </ul>
    </div>
</div>
    